{
  "name": "vue-release-notes",
  "version": "0.1.6",
  "description": "A Vue.js Release Notes Component",
  "author": "Carl St-Laurent",
  "main": "dist/vue-release-notes.js",
  "license": "Apache-2.0",
  "scripts": {
    "dist": "vue build ./src/index.js --config ./build.config.js --dist ./dist/ --prod --lib",
    "dist:demo": "vue build ./src/demo.vue --config ./build.config.js --dist ./demo/ --prod",
    "build": "npm run dist -- --disable-compress",
    "start": "npm run dev",
    "dev": "vue build ./src/demo.vue --config ./build.config.js",
    "demo:open": "open 'http://localhost:3000/'; serve ./demo/"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "vue",
    "component"
  ],
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-html": "6.0.3",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "node-sass": "4.14.1",
    "pug": "3.0.0",
    "sass-loader": "9.0.3",
    "vue-cli": "2.9.6"
  }
}
